{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-prototype-builtins": "off",
      "indent": [ "error", 4 ],
      "brace-style": [ "error", "allman" ],
      "max-len": [ "warn", { "code": 130 } ]
    }
}
